{
  "name": "ollama-ocr",
  "version": "0.0.2",
  "description": "Implementing OCR with a local visual model run by ollama.",
  "author": "bytefer",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
